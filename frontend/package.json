{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "scss:react": "node-sass sass/main.scss src/App.css -w --source-map true",
    "scss:ui": "node-sass sass/main.scss ui/assets/css/style.css -w --source-map true",
    "watch:sass": "npm-run-all --parallel scss:react scss:ui"
  },
  "dependencies": {
    "@ionic/react": "^7.5.2",
    "@reduxjs/toolkit": "^1.9.7",
    "@teamhanko/hanko-elements": "^0.9.0",
    "axios": "^1.6.0",
    "classnames": "^2.3.2",
    "ionicons": "^7.2.1",
    "lodash-es": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.3",
    "react-router-dom": "^6.17.0",
    "redux": "^4.2.1"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.10",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
